{
    "manifest_version": 3,
    "name": "custom new tab",
    "description": "\"Fork\" of minimal newtab. Essentially a rewrite with more customizability.",
    "version": "0.1.0",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "options_page": "options.html",
    "permissions": [
        "bookmarks",
        "geolocation",
        "storage"
    ]
}
